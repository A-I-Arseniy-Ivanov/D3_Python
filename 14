#14.3 import math
a=int(input())
def TrianglePS(a):
  if a<=0:
    print("Ошибка")
  else:
    P=3*a
    S=(3**(1/2))*a**2/4
  return P, S
perimeter, square = TrianglePS(a)
print(f"P = {perimeter}, S ={square}")

