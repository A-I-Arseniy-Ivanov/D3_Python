#14.2
import math
R=int(input())
def Circle(R):
  if R<=0:
    print("Ошибка")
  else:
    S=math.pi*R**2
    L=2*math.pi*R
  return R, L
square, length = Circle(R)
print(f"S = {square}, L ={length}")

