#14.1
import math
X=int(input())
Y=int(input())
def Mean(X, Y):
  if X <= 0 or Y <= 0:
    print("Оба числа должны быть положительными.")
  else:
    AMean = (X + Y)/2
    GMean = X * Y
  return AMean, GMean
arithmetic, geometric = Mean(X, Y)
print(f"AMean = {arithmetic}, GMean = {geometric}")

