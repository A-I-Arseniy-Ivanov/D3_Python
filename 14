#14.5
def RectPS(x1, y1, x2, y2):
    a = abs(x2 - x1)
    b = abs(y2 - y1)
    
    P = 2 * (a + b)
    S = a * b
   return P, S
RectPS(1, 1, 4, 5)
    print(f"Периметр прямоугольника P = {P}")
    print(f"Площадь прямоугольника S = {S}")

