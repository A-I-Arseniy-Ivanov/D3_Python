#14.15
def IsLeapYear(Y):
    if Y % 4 == 0:
        return "Год ВИсокосный"
    elif (Y % 100 == 0 and Y % 400 != 0) or Y % 4 != 0:
        return "Год HeВИсокосный"
print(IsLeapYear(2025))
