#14.4 import math
R1=int(input())
R2=int(input())
def RingS(R1, R2):
  if R1<=0 and R2<=0 and R1<=R2:
    print("Ошибка")
  else:
    S=math.pi*(R1**2-R2**2)
  return S
square = RingS(R1,R2)
print(f"S={square}")

